<% if @user.handymen %>
   <h1> Handyman </h1>
   <%= link_to "Approve mission",class:'btn btn-primary' %>
<% else %>
    <h1> <%= @user.first_name %> Dashboard </h1>
    <ul> <% @user.missions.each do |mission|%>
      <li>Mission Start Date:<%= mission.start_date %>
      Mission description: <%= mission.description %></li>
    </ul>
    <% end %>
<% end %>
  