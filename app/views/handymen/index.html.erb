<div class="container">
	<div>
		<h1>LIST OF THE HANDYMEN NEAR YOU</h1>
 <div class="columns">
  <ul class="price">
<!--     <li class="header">Basic</li> -->
		<% @handymen.each do |handyman| %>
    <li class="grey"><%= handyman.user.first_name %> <%= handyman.user.last_name %></li>
    <li><%= image_tag "mechanic.png", height: "30", width: "30" %><%= handyman.job_title %></li>
    <li><%= image_tag "best-price.png", height: "30", width: "30" %><%= handyman.average_price %></li>
    <li>10 Domains</li>
    <li>1GB Bandwidth</li>
    <li class="grey"><%= link_to "SEE MORE", handyman_path(handyman),class:'btn btn-primary' %></li>
 		<% end %>
  </ul>
</div>
<div class="container">
<div id="map"
     style="width: 600px; height: 600px;"
     data-markers="<%= @markers.to_json %>"
     data-mapbox-api-key="pk.eyJ1Ijoia2F2ZWVzaCIsImEiOiJja3Jlazd0c3kxc205MnZxdTd0N2twOXAwIn0.My3-kyt0F3aF2jkksJkxCw"></div>
</div>
